<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<bean id="Currency" class="com.egitim.spring.model.Currency" scope="prototype">
	
	</bean>
	
	<bean id="PriceChange" class="com.egitim.spring.model.PriceChange" scope="prototype">
	<constructor-arg name="currencyName" value="USDTRY" type = "java.lang.String"/>
	</bean>
</beans>